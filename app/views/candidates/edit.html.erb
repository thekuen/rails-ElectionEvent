<section class="p-5 mx-auto mt-6 bg-gray-300 w-9/12 rounded-xl">
  <h1>編輯候選人資料</h1>

  <%= form_with(model: @candidate, data: { turbo: false}) do |f| %>

  <%= render "form", f: f  %>

  <div class=" gap-20 my-4">
    <%= f.submit "編輯完成送出", class:"cursor-pointer" %>
    <%= link_to '返回候選人列表', candidates_path %>
  </div>




  <% end %>




</section>



